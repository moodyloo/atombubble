{
  "name": "atombubble",
  "main": "./lib/atombubble",
  "version": "1.0.0",
  "description": "Code Bubbles for the Atom Editor",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "atombubble:toggle"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/plugin-proposal-class-properties",{"loose": true}]
    ]
  },
  "jest": {
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest"
  },
  "deserializers": {
    "atombubble/AtomBubbleView": "deserializeAtomBubbleView"
  },
  "repository": "https://github.com/atom/atombubble",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.2",
    "acorn-jsx": "^5.0.1",
    "acorn-loose": "^6.0.0",
    "atom-languageclient": "^0.9.9",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": ">=3.4.1",
    "ejs": "^2.6.1",
    "electron": "^4.0.5",
    "electron-rebuild": "^1.8.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "escodegen": "^1.11.1",
    "esprima": "^4.0.1",
    "etch": "^0.14.0",
    "glob": "^7.1.3",
    "interactjs": "^1.3.4",
    "jest": "^24.5.0",
    "jquery": "^3.3.1",
    "load-json-file": "^5.2.0",
    "local-storage": "^1.4.2",
    "promise": "^8.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1",
    "tree-sitter": "^0.13.23",
    "uuid": "^3.3.2",
    "webpack": "^4.29.3",
    "write-json-file": "^3.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "webpack-cli": "^3.2.3"
  }
}
